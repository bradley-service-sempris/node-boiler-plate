{
  "name": "node-boiler-plate",
  "version": "0.0.2",
  "description": "boiler plate for node apps ft. TS",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "tslint --project ./tsconfig.json",
    "start:development": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "start": "node src/index.js",
    "compile": "tsc --project ./"
  },
  "author": "bradley service",
  "license": "ISC",
  "dependencies": {
    "axios": "0.21.4",
    "config": "3.3.6",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@sempris/eslint-config": "0.0.9",
    "@types/config": "0.0.39",
    "@types/express": "4.17.13",
    "@types/node": "16.10.1",
    "nodemon": "2.0.12",
    "typescript": "4.4.2",
    "prettier": "2.4.1",
    "ts-node": "10.2.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0"
  }
}
